<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.model.Users" table="USERS">
		<id name="id" column="u_id"></id>
		<property name="username" column="u_username" />
		<property name="password" column="u_password"></property>
		<property name="firstname" column="u_firstname"></property>		
		<property name="lastname" column="u_lastname"></property>	
		<property name="email" column="u_email"></property>	
		<property name="userRoleID" column="ur_id"></property>	
	</class>
</hibernate-mapping>